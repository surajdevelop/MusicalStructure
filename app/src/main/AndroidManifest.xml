<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musicalstructure">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SongsActivity"
            android:label="@string/category_songs" />
        <activity
            android:name=".PlaylistActivity"
            android:label="@string/category_playlist" />
        <activity
            android:name=".AlbumsActivity"
            android:label="@string/category_albums" />
        <activity
            android:name=".ArtistsActivity"
            android:label="@string/category_artist" />
        <activity
            android:name=".PurposeActivity"
            android:label="Purpose" />
        <activity
            android:name=".ViewsActivity"
            android:label="Views" />
        <activity
            android:name=".XxActivity"
            android:label="X" />
        <activity
            android:name=".DrakeActivity"
            android:label="Drake" />
        <activity
            android:name=".EdActivity"
            android:label="Ed Sheeran" />
        <activity
            android:name=".JustinActivity"
            android:label="Justin Bieber" />
        <activity android:name=".RecentActivity"
            android:label="Recently Added"/>
        <activity android:name=".FrequentActivity"
            android:label="Frequently Played"></activity>
    </application>

</manifest>